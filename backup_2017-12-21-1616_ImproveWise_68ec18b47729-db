# Table: `wp_eg_navigation_skins`
# Approximate rows expected in table: 16

# Delete any existing table `wp_eg_navigation_skins`

DROP TABLE IF EXISTS `wp_eg_navigation_skins`;

# Table structure of table `wp_eg_navigation_skins`

CREATE TABLE `wp_eg_navigation_skins` (
  `id` mediumint(6) NOT NULL AUTO_INCREMENT,
  `name` varchar(191) COLLATE utf8mb4_unicode_520_ci NOT NULL,
  `handle` varchar(191) COLLATE utf8mb4_unicode_520_ci NOT NULL,
  `css` mediumtext COLLATE utf8mb4_unicode_520_ci NOT NULL,
  UNIQUE KEY `id` (`id`),
  UNIQUE KEY `handle` (`handle`)
